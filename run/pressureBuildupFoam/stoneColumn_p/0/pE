FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pE;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    seaBed
    {
	type            fixedValue;
	value           uniform 0;	
    }
 
    sides
    {
        type            cyclic;   
    }
    
    soilDomainBottom
    {
        type            zeroGradient;
    }
    
    frontBack
    {
        type  		 empty;
    }
}

