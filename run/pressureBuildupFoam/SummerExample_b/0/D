FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    seaBed
    {
        type            tractionDisplacement;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    sides
    {
        type            cyclic;   
    }
    soilDomainBottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
     frontBack
    {
        type  		 empty;
    }
    
    stoneColumn
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

