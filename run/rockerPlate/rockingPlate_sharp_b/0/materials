FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      materials;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    seaBed
    {
        type            zeroGradient;
    }
    frontBack
    {
        type            empty;
    }
    soilDomainBottom
    {
        type            zeroGradient;
    }
    
    rockerTop
    {
        type            zeroGradient;   
    }

    rocker
    {
        type            zeroGradient;   
    }    
    
    
    "left|right"
    {
        type            zeroGradient;   
    }
}


