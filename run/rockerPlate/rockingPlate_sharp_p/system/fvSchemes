FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default 	    Euler;
}

gradSchemes
{
    default         leastSquares;
    grad(U)         leastSquares;
    grad(p)         leastSquares;
}

divSchemes
{
    default         none;
    div(sigmaU)     Gauss linear;
    div(ddt(Dp2,U)) Gauss linear corrected;
    div(ddt(Dp3,U)) Gauss linear corrected;
    div(d2dt2(U))   Gauss linear corrected;
}

laplacianSchemes
{
    default         none;
    laplacian(DD,U) Gauss linear corrected;
    laplacian(Dp,p) Gauss harmonic corrected;
    laplacian(Dp2,p) Gauss harmonic corrected;
    laplacian(Dp3,p) Gauss harmonic corrected;
    laplacian(cv,pE) Gauss linear corrected;

}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         none;
}

fluxRequired
{
    default         no;
    D               yes;
    p               no;
}


// ************************************************************************* //
